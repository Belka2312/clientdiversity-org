{
    "version": "0.2.0",
    "configurations": [

        
        {
            "type": "chrome",
            "request": "launch",
            "name": "Запустить Chrome на localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Запустить сценарий: test",
            "request": "launch",
            "command": "npm run test",
            "cwd": "${workspaceFolder}"
        }
    ]
}